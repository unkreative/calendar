
// import student_entreprise

$student_entreprise_id = array();
$student_entreprise_student_id = array();
$student_entreprise_entreprise_id = array();
$student_entreprise_plage_1 = array();
$student_entreprise_plage_2 = array();
$student_entreprise_plage_3 = array();
$student_entreprise_plage_4 = array();
$student_entreprise_plage_5 = array();
$student_entreprise_plage_6 = array();
$student_entreprise_plage_7 = array();
$student_entreprise_plage_8 = array();
$student_entreprise_year = array();

$arr_student_entreprise = array(
  array("id", $student_entreprise_id),
  array("student_id", $student_entreprise_student_id),
  array("entreprise_id", $student_entreprise_entreprise_id),
  array("plage_1", $student_entreprise_plage_1),
  array("plage_2", $student_entreprise_plage_2),
  array("plage_3", $student_entreprise_plage_3),
  array("plage_4", $student_entreprise_plage_4),
  array("plage_5", $student_entreprise_plage_5),
  array("plage_6", $student_entreprise_plage_6),
  array("plage_7", $student_entreprise_plage_7),
  array("plage_8", $student_entreprise_plage_8),
  array("year", $student_entreprise_year),
);

$sql_student_entreprise = "SELECT id, student_id, entreprise_id, plage_1, plage_2, plage_3, plage_4, plage_5, plage_6, plage_7, plage_8, year FROM student_entreprise";

$result_student_entreprise = import_arr($sql_student_entreprise, $arr_student_entreprise);

$student_entreprise_id = $result_student_entreprise[0][1];
$student_entreprise_student_id = $result_student_entreprise[1][1];
$student_entreprise_entreprise_id = $result_student_entreprise[2][1];
$student_entreprise_plage_1 = $result_student_entreprise[3][1];
$student_entreprise_plage_2 = $result_student_entreprise[4][1];
$student_entreprise_plage_3 = $result_student_entreprise[5][1];
$student_entreprise_plage_4 = $result_student_entreprise[6][1];
$student_entreprise_plage_5 = $result_student_entreprise[7][1];
$student_entreprise_plage_6 = $result_student_entreprise[8][1];
$student_entreprise_plage_7 = $result_student_entreprise[9][1];
$student_entreprise_plage_8 = $result_student_entreprise[10][1];
$student_entreprise_year = $result_student_entreprise[11][1];
