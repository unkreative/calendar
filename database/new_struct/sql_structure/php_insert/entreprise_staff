
// import entreprise_staff

$entreprise_staff_id = array();
$entreprise_staff_staff_id = array();
$entreprise_staff_entreprise_id = array();
$entreprise_staff_post = array();
$entreprise_staff_year = array();

$arr_entreprise_staff = array(
  array("id", $entreprise_staff_id),
  array("staff_id", $entreprise_staff_staff_id),
  array("entreprise_id", $entreprise_staff_entreprise_id),
  array("post", $entreprise_staff_post),
  array("year", $entreprise_staff_year),
);

$sql_entreprise_staff = "SELECT id, staff_id, entreprise_id, post, year FROM entreprise_staff";

$result_entreprise_staff = import_arr($sql_entreprise_staff, $arr_entreprise_staff);

$entreprise_staff_id = $result_entreprise_staff[0][1];
$entreprise_staff_staff_id = $result_entreprise_staff[1][1];
$entreprise_staff_entreprise_id = $result_entreprise_staff[2][1];
$entreprise_staff_post = $result_entreprise_staff[3][1];
$entreprise_staff_year = $result_entreprise_staff[4][1];
